<div class="container">
  <div class="row">

    <!-- Side bar -->
    <div class="three columns">


      <div class="row build">
        <div class="twelve columns hide-on-phones">


          <img src="/img/steal-rocket.jpg">
          <h5 class="subheader"><%= description %></h5>

          <p>
            <a href="/upload/<%= project.id %>" class="large blue button radius">
              Upload Build
            </a>
          </p>
          <p>
            <a href="">Edit Project Settings</a>
          </p>

          <!--
          <hr/>

          <p>Project admin @seymores</p>
