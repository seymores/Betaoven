<div class="container">

  <div class="row show-on-phones">
    <div class="twelve columns">
      <h6 class="subheader"><a href="">beta_oven1.apk, version: 2.3</a></h6>
      <a class="large red button radius" href="#">Download</a>
      <p> 623kb Uploaded <strong>4 hours ago.</strong></p>
      <br/>
    </div>
  </div>

  <div class="row">

    <!-- Side bar -->
    <div class="three columns">

      <div class="row">
        <div class="twelve columns hide-on-phones">
          <!--
          <img src="http://placehold.it/100x100">
          -->
          <h6 class="subheader">
            <%= build.filename %><%= build.version_code ? ', version: '+build.version_code : '' %>
          </h6>
          <p>
            <%= build.changelog %>
          </p>

          <p>
            <%= build.prettySize %> Uploaded
            <strong>
              <%= build.prettyCreatedAt %>.
            </strong>
          </p>


          <span class="green radius label">
            <%= build.numApproved %> ppls approved
          </span>
          <br />
          <span class="red radius label">
            <%= build.numDisapproved %> ppls disapproved
          </span>
          <br />
          <span class="black radius label">
            <%= build.numNoFeedback %> no feedback
          </span>
          <br />
          <span class="white radius label">
            <%= build.numDownloads %> downloads
          </span>

          <!--
          <hr/>
          <p> Project admin @seymores </p>
          <p><Strong>Project members</strong></p>
